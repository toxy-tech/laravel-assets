{
    "name": "toxy-tech/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "toxytech",
        "toxytech cms",
        "toxytech platform",
        "toxytech assets"
    ],
    "homepage": "https://toxytech.com",
    "authors": [
        {
            "name": "Adham Elsharkawy",
            "email": "adhamshrqawy@icloud.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0|^8.1|^8.2|^8.3",
        "laravel/framework": "^5.6|^6|^7|^8|^9|^10|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0|^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ToxyTech\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ToxyTech\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ToxyTech\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "ToxyTech\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
